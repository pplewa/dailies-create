<div>
	<h3 style="color: #205081; margin: 10px 0; text-align: center;">{{title}}</h3>
	<table style="width: 100%; min-width: 100%; max-width: 100%; border: 1px #ddd solid;" cellpadding="0" cellspacing="0">
		<thead>
			<tr>
				<th style="width:33%;padding: 5px 10px; color: #3A71B0;border: 1px #ddd solid;">{{list1.name}}</th>
				<th style="width:33%;padding: 5px 10px; color: #3A71B0;border: 1px #ddd solid;">{{list2.name}}</th>
				<th style="width:33%;padding: 5px 10px; color: #3A71B0;border: 1px #ddd solid;">{{list3.name}}</th>
			</tr>
		</thead>
		<tbody>
			<tr>
				<td style="width:33%;padding: 5px 0;border: 1px #ddd solid;">
					<ul>{{#each list1.cards}}<li>{{name}}</li>{{/each}}</ul>
				</td>
				<td style="width:33%;padding: 5px 0;border: 1px #ddd solid;">
					<ul>{{#each list2.cards}}<li>{{name}}</li>{{/each}}</ul>
				</td>
				<td style="width:33%;padding: 5px 0;border: 1px #ddd solid;">
					<ul>{{#each list3.cards}}<li>{{name}}</li>{{/each}}</ul>
				</td>
			</tr>
		</tbody>
	</table>
	<img src="https://upload.wikimedia.org/wikipedia/commons/c/c0/Blank.gif" />
</div>